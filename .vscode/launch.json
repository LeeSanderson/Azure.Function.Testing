{
    "version": "0.2.0",
    "configurations": [        
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Tests/Azure.Functions.Testing.Tests/bin/Debug/net7.0/Azure.Functions.Testing.Tests.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Tests/Azure.Functions.Testing.Tests",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        },
        {
            "name": "Attach to .NET Functions",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:azureFunctions.pickProcess}"
        }
    ]
}